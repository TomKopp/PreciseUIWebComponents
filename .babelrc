{
  "presets": [
    "@babel/env",
    "@babel/typescript",
  ],
  "plugins": [
    "@babel/plugin-proposal-numeric-separator"
    , ["@babel/plugin-proposal-decorators", { "decoratorsBeforeExport": true }]
    , "@babel/plugin-proposal-class-properties"
    , "@babel/plugin-proposal-object-rest-spread"
  ],
  "env": {
    "production": {
      "presets": ["minify"]
    }
  }
}
