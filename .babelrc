{
  "presets": [
    "@babel/env",
    "@babel/typescript",
  ],
  "plugins": [
    "@babel/plugin-proposal-numeric-separator",
    ["@babel/plugin-proposal-decorators", { "decoratorsBeforeExport": true }],
    "@babel/proposal-class-properties",
    "@babel/proposal-object-rest-spread"
  ],
  "env": {
    "production": {
      "presets": ["minify"]
    }
  }
}
